{
  "routes": [
    {
      "route": "/locations",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/locations/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/map",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/admin",
      "allowedRoles": ["admin", "authenticated"]
    }
  ],

  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg}"]
  }
}
